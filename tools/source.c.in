#include "mylib.h"
#include "tlpi_hdr.h"

#include <fcntl.h>
#include <string.h>
#include <sys/stat.h>
#include <sys/types.h>

void usage_error(const char *progname)
{
    usageErr("%s\n", progname);
}


int main(int argc, char *argv[])
{
    if (argc < 1) {
        fprintf(stderr, "%s: too few arguments\n", argv[0]);
        usage_error(argv[0]);
    }

    return 0;
}
